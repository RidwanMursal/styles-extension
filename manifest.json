{
    "name": "html saver", 
    "version": "0.1", 
    "manifest_version": 2, 
    "description": "desc", 
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["index.js"],
            "css": ["styles.css"]
           
        }
    ], 
    "background": {
        "scripts": ["backgroundScript.js"]
    },
    "browser_action": {
        "default_icon": "iconex.png"
    }, 
    "permissions": [     "tabs", "storage"   ], 
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

    
    
    
}

